      <orm:basic name="level" optional="true"/>
      <orm:basic name="time" optional="true">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="host" optional="true"/>
      <orm:basic name="service" optional="true"/>
      <orm:basic name="serviceClass" optional="true"/>
      <orm:basic name="message" optional="true"/>
      <orm:basic name="category" optional="true"/>
      <orm:basic name="userName" optional="true"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="vortex.model.AuthChallenge" name="AuthChallenge">
    <orm:attributes>
      <orm:basic name="question" optional="true"/>
      <orm:basic name="answer" optional="true"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="vortex.model.Role" name="Role">
    <orm:attributes>
      <orm:basic name="name" optional="true"/>
      <orm:basic name="description" optional="true"/>
      <orm:basic name="origin" optional="true"/>
      <orm:many-to-many name="permissions" target-entity="vortex.model.Permission">
        <orm:order-column/>
        <orm:join-table name="Role_Permissions"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
      <orm:many-to-many name="subRoles" target-entity="vortex.model.Role">
        <orm:order-column/>
        <orm:join-table name="Role_SubRoles"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="vortex.model.Password" name="Password">
    <orm:attributes>
      <orm:basic name="hash" optional="true"/>
      <orm:basic name="salt" optional="true"/>
      <orm:basic name="created" optional="true">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="vortex.model.Permission" name="Permission">
    <orm:attributes>
      <orm:basic name="name" optional="true"/>
      <orm:basic name="description" optional="true"/>
      <orm:basic name="type" optional="true"/>
      <orm:basic name="origin" optional="true"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="vortex.model.Job" name="Job">
    <orm:attributes>
      <orm:basic name="type" optional="true"/>
      <orm:basic name="name" optional="true"/>
      <orm:basic name="description" optional="true"/>
      <orm:basic name="assetID" optional="true"/>
      <orm:basic name="state" optional="true"/>
      <orm:basic name="status" optional="true"/>
      <orm:basic name="source" optional="true"/>
      <orm:basic name="created" optional="true">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="runInterval" optional="true"/>
      <orm:basic name="startTime" optional="true">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="lastAttempt" optional="true">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="completed" optional="true">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
    </orm:attributes>
