<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.inventory.PType" name="PType">
    <orm:attributes>
      <orm:basic name="id" optional="true"/>
      <orm:basic name="name" optional="true"/>
      <orm:many-to-one name="base" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.inventory.PType">
        <orm:join-table name="PType_base"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:one-to-many mapped-by="base" name="subTypes" target-entity="org.eclipse.emf.texo.test.model.samples.inventory.PType">
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:many-to-many name="subNOTypes" target-entity="org.eclipse.emf.texo.test.model.samples.inventory.PType">
        <orm:join-table name="PType_subNOTypes"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
      <orm:many-to-many name="infoReferences" target-entity="org.eclipse.emf.texo.test.model.samples.inventory.PDeclaration">
        <orm:order-column/>
        <orm:join-table name="PType_infoReferences"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.inventory.PDeclaration" name="PDeclaration">
    <orm:attributes>
      <orm:basic name="id" optional="true"/>
      <orm:basic name="name" optional="true"/>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>