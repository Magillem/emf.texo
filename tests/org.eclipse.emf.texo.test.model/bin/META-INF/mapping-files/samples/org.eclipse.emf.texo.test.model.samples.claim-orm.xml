<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.claim.Claim" name="Claim">
    <orm:attributes>
      <orm:basic name="billingName" optional="false"/>
      <orm:basic name="claimGroupVSPId" optional="false"/>
      <orm:basic name="claimStatus" optional="false"/>
      <orm:many-to-one name="claimCompositeKey" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.claim.ClaimCompositeKey">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:one-to-many name="claimLine" orphan-removal="true" target-entity="org.eclipse.emf.texo.test.model.samples.claim.ClaimLine">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.claim.ClaimCompositeKey" name="ClaimCompositeKey">
    <orm:attributes>
      <orm:basic name="claimNumber" optional="false"/>
      <orm:basic name="claimExtensionNumber" optional="false"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.claim.ClaimLine" name="ClaimLine">
    <orm:attributes>
      <orm:basic name="claimLineBilledAmount" optional="false"/>
      <orm:basic name="serviceCode" optional="false"/>
      <orm:basic name="claimLineStatus" optional="false"/>
      <orm:many-to-one name="claim" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.claim.Claim">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="claimLineCompositeKey" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.claim.ClaimLineCompositeKey">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.claim.ClaimLineCompositeKey" name="ClaimLineCompositeKey">
    <orm:attributes>
      <orm:basic name="claimLineNumber" optional="false"/>
      <orm:many-to-one name="claim" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.claim.Claim">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>