<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.schemaprimerpo.PurchaseOrder" name="PurchaseOrder">
    <orm:attributes>
      <orm:basic name="comment" optional="true"/>
      <orm:basic name="orderDate" optional="true">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:many-to-one name="shipTo" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.schemaprimerpo.USAddress">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="billTo" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.schemaprimerpo.USAddress">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:one-to-many name="items" target-entity="org.eclipse.emf.texo.test.model.samples.schemaprimerpo.Item">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.schemaprimerpo.Item" name="Item">
    <orm:attributes>
      <orm:basic name="productName" optional="false"/>
      <orm:basic name="quantity" optional="false"/>
      <orm:basic name="uSPrice" optional="false"/>
      <orm:basic name="comment" optional="true"/>
      <orm:basic name="shipDate" optional="true">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="partNum" optional="false"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.schemaprimerpo.USAddress" name="USAddress">
    <orm:attributes>
      <orm:basic name="name" optional="false"/>
      <orm:basic name="street" optional="false"/>
      <orm:basic name="city" optional="false"/>
      <orm:basic name="state" optional="false"/>
      <orm:basic name="zip" optional="false"/>
      <orm:basic name="country" optional="true"/>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>