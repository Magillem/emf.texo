<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.sunbooks.AuthorsType" name="AuthorsType">
    <orm:table name="AuthorsType"/>
    <orm:attributes>
      <orm:element-collection name="authorName">
        <orm:column name="element"/>
        <orm:collection-table name="AuthorsType_authorName">
          <orm:join-column name="AuthorsType_id"/>
        </orm:collection-table>
      </orm:element-collection>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.sunbooks.BooksType" name="BooksType">
    <orm:table name="BooksType"/>
    <orm:attributes>
      <orm:one-to-many name="book">
        <orm:join-column name="BooksType_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.sunbooks.BookType" name="BookType">
    <orm:table name="BookType"/>
    <orm:attributes>
      <orm:basic name="name" optional="false">
        <orm:column name="name"/>
      </orm:basic>
      <orm:basic name="iSBN" optional="false">
        <orm:column name="iSBN"/>
      </orm:basic>
      <orm:basic name="price" optional="false">
        <orm:column name="price"/>
      </orm:basic>
      <orm:basic name="description">
        <orm:column name="description"/>
      </orm:basic>
      <orm:basic name="publicationDate" optional="false">
        <orm:column name="publicationDate"/>
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="bookCategory" optional="false">
        <orm:column name="bookCategory"/>
      </orm:basic>
      <orm:basic name="itemId">
        <orm:column name="itemId"/>
      </orm:basic>
      <orm:many-to-one name="authors" optional="false">
        <orm:join-column name="authors"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="promotion" optional="false">
        <orm:join-column name="promotion"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.sunbooks.PromotionType" name="PromotionType">
    <orm:table name="PromotionType"/>
    <orm:attributes>
      <orm:basic name="discount">
        <orm:column name="discount"/>
      </orm:basic>
      <orm:basic name="none">
        <orm:column name="none"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.sunbooks.CollectionType" name="CollectionType">
    <orm:table name="CollectionType"/>
    <orm:attributes>
      <orm:many-to-one name="books" optional="false">
        <orm:join-column name="books"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>