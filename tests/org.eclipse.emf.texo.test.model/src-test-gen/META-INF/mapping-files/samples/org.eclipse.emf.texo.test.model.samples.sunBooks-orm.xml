<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.sunbooks.AuthorsType" name="AuthorsType">
    <orm:attributes>
      <orm:element-collection name="authorName">
        <orm:order-column/>
        <orm:collection-table name="AuthorsType_authorName"/>
      </orm:element-collection>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.sunbooks.BooksType" name="BooksType">
    <orm:attributes>
      <orm:one-to-many name="book" target-entity="org.eclipse.emf.texo.test.model.samples.sunbooks.BookType">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.sunbooks.BookType" name="BookType">
    <orm:attributes>
      <orm:basic name="name" optional="false"/>
      <orm:basic name="iSBN" optional="false"/>
      <orm:basic name="price" optional="false"/>
      <orm:basic name="description" optional="true"/>
      <orm:basic name="publicationDate" optional="false">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="bookCategory" optional="false"/>
      <orm:basic name="itemId" optional="true"/>
      <orm:many-to-one name="authors" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.sunbooks.AuthorsType">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="promotion" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.sunbooks.PromotionType">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.sunbooks.PromotionType" name="PromotionType">
    <orm:attributes>
      <orm:basic name="discount" optional="true"/>
      <orm:basic name="none" optional="true"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.sunbooks.CollectionType" name="CollectionType">
    <orm:attributes>
      <orm:many-to-one name="books" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.sunbooks.BooksType">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>