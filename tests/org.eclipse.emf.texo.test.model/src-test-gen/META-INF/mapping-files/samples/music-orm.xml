<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.music.Artist" name="Artist">
    <orm:attributes>
      <orm:basic name="id" optional="true"/>
      <orm:basic name="version" optional="true"/>
      <orm:basic name="firstName" optional="true"/>
      <orm:basic name="lastName" optional="true"/>
      <orm:basic name="birthDate" optional="true">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:many-to-one name="country" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.music.Country">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="genre" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.music.Genre">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.music.Country" name="Country">
    <orm:attributes>
      <orm:basic name="code" optional="false"/>
      <orm:basic name="version" optional="true"/>
      <orm:basic name="name" optional="true"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.music.Genre" name="Genre">
    <orm:attributes>
      <orm:basic name="id" optional="true"/>
      <orm:basic name="version" optional="true"/>
      <orm:basic name="name" optional="false"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.music.Album" name="Album">
    <orm:attributes>
      <orm:basic name="id" optional="true"/>
      <orm:basic name="version" optional="true"/>
      <orm:basic name="name" optional="true"/>
      <orm:basic name="releaseDate" optional="true">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:many-to-one name="artist" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.music.Artist">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:one-to-many name="songs" orphan-removal="true" target-entity="org.eclipse.emf.texo.test.model.samples.music.Song">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:many-to-many name="genres" target-entity="org.eclipse.emf.texo.test.model.samples.music.Genre">
        <orm:order-column/>
        <orm:join-table name="Album_genres"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.music.Song" name="Song">
    <orm:attributes>
      <orm:basic name="id" optional="true"/>
      <orm:basic name="version" optional="true"/>
      <orm:basic name="track" optional="true"/>
      <orm:basic name="name" optional="true"/>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>