<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.Book" name="Book">
    <orm:attributes>
      <orm:basic name="title" optional="true"/>
      <orm:basic name="pages" optional="true"/>
      <orm:basic name="category" optional="true"/>
      <orm:many-to-one name="author" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.Writer">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.Item" name="Item">
    <orm:attributes>
      <orm:basic name="publicationDate" optional="true">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity access="PROPERTY" class="org.eclipse.emf.texo.test.model.samples.extlibrary.Lendable" name="Lendable">
    <orm:attributes>
      <orm:basic name="copies" optional="false"/>
      <orm:many-to-many mapped-by="borrowed" name="borrowers" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.Borrower">
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.CirculatingItem" name="CirculatingItem">
    <orm:attributes>
      <orm:basic name="copies" optional="false"/>
      <orm:many-to-many mapped-by="borrowed" name="borrowers" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.Borrower">
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.Writer" name="Writer">
    <orm:attributes>
      <orm:one-to-many name="books" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.Book">
        <orm:order-column/>
        <orm:join-table name="Writer_books"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity access="PROPERTY" class="org.eclipse.emf.texo.test.model.samples.extlibrary.Addressable" name="Addressable">
    <orm:attributes>
      <orm:basic name="address" optional="true"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.Person" name="Person">
    <orm:attributes>
      <orm:basic name="address" optional="true"/>
      <orm:basic name="firstName" optional="false"/>
      <orm:basic name="lastName" optional="false"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.LibraryPeopleFeatureGroup" name="Library_people">
    <orm:attributes>
      <orm:basic name="feature" optional="false">
        <orm:enumerated>STRING</orm:enumerated>
      </orm:basic>
      <orm:many-to-one name="writers" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.Writer">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="employees" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.Employee">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="borrowers" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.Borrower">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.Library" name="Library">
    <orm:attributes>
      <orm:basic name="address" optional="true"/>
      <orm:basic name="name" optional="true"/>
      <orm:many-to-one name="parentBranch" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.Library">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:one-to-many name="people" orphan-removal="true" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.LibraryPeopleFeatureGroup">
        <orm:join-table/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="stock" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.Item">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="branches" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.Library">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:many-to-many name="books" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.Book">
        <orm:join-table name="Library_books"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.Employee" name="Employee">
    <orm:attributes>
      <orm:many-to-one name="manager" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.Employee">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.Borrower" name="Borrower">
    <orm:attributes>
      <orm:many-to-many name="borrowed" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.Lendable">
        <orm:order-column/>
        <orm:join-table name="Borrower_borrowed"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.Periodical" name="Periodical">
    <orm:attributes>
      <orm:basic name="title" optional="true"/>
      <orm:basic name="issuesPerYear" optional="false"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.AudioVisualItem" name="AudioVisualItem">
    <orm:attributes>
      <orm:basic name="title" optional="true"/>
      <orm:basic name="minutesLength" optional="false"/>
      <orm:basic name="damaged" optional="true"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.BookOnTape" name="BookOnTape">
    <orm:attributes>
      <orm:many-to-one name="reader" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.Person">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="author" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.Writer">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.VideoCassette" name="VideoCassette">
    <orm:attributes>
      <orm:many-to-many name="cast" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.Person">
        <orm:order-column/>
        <orm:join-table name="VideoCassette_cast"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>