<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.Book" name="Book">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes>
      <orm:basic name="title">
        <orm:column name="title"/>
      </orm:basic>
      <orm:basic name="pages">
        <orm:column name="pages"/>
      </orm:basic>
      <orm:basic name="category">
        <orm:column name="category"/>
      </orm:basic>
      <orm:many-to-one name="author" optional="false">
        <orm:join-column name="author"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.Item" name="Item">
    <orm:table name="Item"/>
    <orm:attributes>
      <orm:basic name="publicationDate">
        <orm:column name="publicationDate"/>
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.CirculatingItem" name="CirculatingItem">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes/>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.Writer" name="Writer">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes>
      <orm:one-to-many name="books">
        <orm:join-table name="Writer_books">
          <orm:join-column name="Writer_id"/>
          <orm:inverse-join-column name="Book_id"/>
        </orm:join-table>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.Person" name="Person">
    <orm:table name="Person"/>
    <orm:attributes>
      <orm:basic name="firstName" optional="false">
        <orm:column name="firstName"/>
      </orm:basic>
      <orm:basic name="lastName" optional="false">
        <orm:column name="lastName"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.LibraryPeopleFeatureGroup" name="Library_people">
    <orm:attributes>
      <orm:basic name="feature" optional="false">
        <orm:enumerated>STRING</orm:enumerated>
      </orm:basic>
      <orm:many-to-one name="writers">
        <orm:join-column name="writers"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="employees">
        <orm:join-column name="employees"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="borrowers">
        <orm:join-column name="borrowers"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.Library" name="Library">
    <orm:table name="Library"/>
    <orm:attributes>
      <orm:basic name="name">
        <orm:column name="name"/>
      </orm:basic>
      <orm:many-to-one name="parentBranch">
        <orm:join-column name="parentBranch"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:one-to-many name="people" orphan-removal="true" target-entity="org.eclipse.emf.texo.test.model.samples.extlibrary.LibraryPeopleFeatureGroup">
        <orm:join-table/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="stock">
        <orm:join-column name="Library_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="branches">
        <orm:join-column name="Library_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:many-to-many access="PROPERTY" name="books">
        <orm:join-table name="Library_books">
          <orm:join-column name="Library_id"/>
          <orm:inverse-join-column name="Book_id"/>
        </orm:join-table>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.Employee" name="Employee">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes>
      <orm:many-to-one name="manager">
        <orm:join-column name="manager"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.Borrower" name="Borrower">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes>
      <orm:many-to-many name="borrowed">
        <orm:join-table name="Borrower_borrowed">
          <orm:join-column name="Borrower_id"/>
          <orm:inverse-join-column name="Lendable_id"/>
        </orm:join-table>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.Periodical" name="Periodical">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes>
      <orm:basic name="title">
        <orm:column name="title"/>
      </orm:basic>
      <orm:basic name="issuesPerYear" optional="false">
        <orm:column name="issuesPerYear"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.AudioVisualItem" name="AudioVisualItem">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes>
      <orm:basic name="title">
        <orm:column name="title"/>
      </orm:basic>
      <orm:basic name="minutesLength" optional="false">
        <orm:column name="minutesLength"/>
      </orm:basic>
      <orm:basic name="damaged">
        <orm:column name="damaged"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.BookOnTape" name="BookOnTape">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes>
      <orm:many-to-one name="reader">
        <orm:join-column name="reader"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="author">
        <orm:join-column name="author"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extlibrary.VideoCassette" name="VideoCassette">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes>
      <orm:many-to-many name="cast">
        <orm:join-table name="VideoCassette_cast">
          <orm:join-column name="VideoCassette_id"/>
          <orm:inverse-join-column name="Person_id"/>
        </orm:join-table>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>