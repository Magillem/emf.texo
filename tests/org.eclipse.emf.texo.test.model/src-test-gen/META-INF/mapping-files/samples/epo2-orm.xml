<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extendedpo2.Item" name="Item">
    <orm:table name="Item"/>
    <orm:attributes>
      <orm:basic name="productName">
        <orm:column name="productName"/>
      </orm:basic>
      <orm:basic name="quantity">
        <orm:column name="quantity"/>
      </orm:basic>
      <orm:basic name="uSPrice">
        <orm:column name="USPrice"/>
      </orm:basic>
      <orm:basic name="comment">
        <orm:column name="comment"/>
      </orm:basic>
      <orm:basic name="shipDate">
        <orm:column name="shipDate"/>
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="partNum">
        <orm:column name="partNum"/>
      </orm:basic>
      <orm:many-to-one name="order" optional="false">
        <orm:join-column name="order"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extendedpo2.PurchaseOrder" name="PurchaseOrder">
    <orm:table name="PurchaseOrder"/>
    <orm:attributes>
      <orm:basic name="comment">
        <orm:column name="comment"/>
      </orm:basic>
      <orm:basic name="orderDate">
        <orm:column name="orderDate"/>
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="status">
        <orm:column name="status"/>
      </orm:basic>
      <orm:many-to-one name="billTo" optional="false">
        <orm:join-column name="billTo"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="shipTo">
        <orm:join-column name="shipTo"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="customer" optional="false">
        <orm:join-column name="customer"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="previousOrder">
        <orm:join-column name="previousOrder"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:one-to-many name="items">
        <orm:join-column name="PurchaseOrder_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extendedpo2.USAddress" name="USAddress">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes>
      <orm:basic name="street">
        <orm:column name="street"/>
      </orm:basic>
      <orm:basic name="city">
        <orm:column name="city"/>
      </orm:basic>
      <orm:basic name="state">
        <orm:column name="state"/>
      </orm:basic>
      <orm:basic name="zip">
        <orm:column name="zip"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extendedpo2.Address" name="Address">
    <orm:table name="Address"/>
    <orm:attributes>
      <orm:basic name="name">
        <orm:column name="name"/>
      </orm:basic>
      <orm:basic name="country">
        <orm:column name="country"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extendedpo2.Customer" name="Customer">
    <orm:table name="Customer"/>
    <orm:attributes>
      <orm:basic name="customerID">
        <orm:column name="customerID"/>
      </orm:basic>
      <orm:one-to-many name="orders">
        <orm:join-table name="Customer_orders">
          <orm:join-column name="Customer_id"/>
          <orm:inverse-join-column name="PurchaseOrder_id"/>
        </orm:join-table>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extendedpo2.Supplier" name="Supplier">
    <orm:table name="Supplier"/>
    <orm:attributes>
      <orm:basic name="name">
        <orm:column name="name"/>
      </orm:basic>
      <orm:one-to-many name="customers">
        <orm:join-column name="Supplier_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="orders">
        <orm:join-column name="Supplier_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extendedpo2.GlobalAddress" name="GlobalAddress">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes>
      <orm:element-collection name="location">
        <orm:column name="element"/>
        <orm:collection-table name="GlobalAddress_location">
          <orm:join-column name="GlobalAddress_id"/>
        </orm:collection-table>
      </orm:element-collection>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extendedpo2.GlobalLocation" name="GlobalLocation">
    <orm:table name="GlobalLocation"/>
    <orm:attributes>
      <orm:basic name="countryCode">
        <orm:column name="countryCode"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>