<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extendedpo2.Item" name="Item">
    <orm:attributes>
      <orm:basic name="productName" optional="true"/>
      <orm:basic name="quantity" optional="true"/>
      <orm:basic name="uSPrice" optional="true"/>
      <orm:basic name="comment" optional="true"/>
      <orm:basic name="shipDate" optional="true">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="partNum" optional="true"/>
      <orm:many-to-one name="order" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.extendedpo2.PurchaseOrder">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extendedpo2.PurchaseOrder" name="PurchaseOrder">
    <orm:attributes>
      <orm:basic name="comment" optional="true"/>
      <orm:basic name="orderDate" optional="true">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="status" optional="true"/>
      <orm:many-to-one name="billTo" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.extendedpo2.Address">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="shipTo" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.extendedpo2.Address">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="customer" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.extendedpo2.Customer">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="previousOrder" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.extendedpo2.PurchaseOrder">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:one-to-many name="items" target-entity="org.eclipse.emf.texo.test.model.samples.extendedpo2.Item">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extendedpo2.USAddress" name="USAddress">
    <orm:attributes>
      <orm:basic name="street" optional="true"/>
      <orm:basic name="city" optional="true"/>
      <orm:basic name="state" optional="true"/>
      <orm:basic name="zip" optional="true"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extendedpo2.Address" name="Address">
    <orm:attributes>
      <orm:basic name="name" optional="true"/>
      <orm:basic name="country" optional="true"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extendedpo2.Customer" name="Customer">
    <orm:attributes>
      <orm:basic name="customerID" optional="true"/>
      <orm:one-to-many name="orders" target-entity="org.eclipse.emf.texo.test.model.samples.extendedpo2.PurchaseOrder">
        <orm:order-column/>
        <orm:join-table name="Customer_orders"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extendedpo2.Supplier" name="Supplier">
    <orm:attributes>
      <orm:basic name="name" optional="true"/>
      <orm:one-to-many name="customers" target-entity="org.eclipse.emf.texo.test.model.samples.extendedpo2.Customer">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="orders" target-entity="org.eclipse.emf.texo.test.model.samples.extendedpo2.PurchaseOrder">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extendedpo2.GlobalAddress" name="GlobalAddress">
    <orm:attributes>
      <orm:element-collection name="location">
        <orm:order-column/>
        <orm:collection-table name="GlobalAddress_location"/>
      </orm:element-collection>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.extendedpo2.GlobalLocation" name="GlobalLocation">
    <orm:attributes>
      <orm:basic name="countryCode" optional="true"/>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>