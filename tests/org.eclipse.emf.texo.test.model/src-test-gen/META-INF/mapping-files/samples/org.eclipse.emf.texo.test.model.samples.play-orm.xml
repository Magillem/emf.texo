<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.play.ActType" name="ActType">
    <orm:attributes>
      <orm:basic name="pindex" optional="false"/>
      <orm:basic name="title" optional="false"/>
      <orm:one-to-many name="scene" target-entity="org.eclipse.emf.texo.test.model.samples.play.SceneType">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.play.SceneTypeGroupFeatureGroup" name="SceneType_group">
    <orm:attributes>
      <orm:basic access="PROPERTY" name="stageDirections" optional="true"/>
      <orm:basic name="feature" optional="false">
        <orm:enumerated>STRING</orm:enumerated>
      </orm:basic>
      <orm:many-to-one access="PROPERTY" name="speech" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.play.SpeechType">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.play.SceneType" name="SceneType">
    <orm:attributes>
      <orm:basic name="pindex" optional="false"/>
      <orm:basic name="title" optional="false"/>
      <orm:one-to-many name="group" orphan-removal="true" target-entity="org.eclipse.emf.texo.test.model.samples.play.SceneTypeGroupFeatureGroup">
        <orm:join-table/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.play.FmType" name="FmType">
    <orm:attributes>
      <orm:element-collection name="p">
        <orm:order-column/>
        <orm:collection-table name="FmType_p"/>
      </orm:element-collection>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.play.PersonaeTypeGroupFeatureGroup" name="PersonaeType_group">
    <orm:attributes>
      <orm:basic access="PROPERTY" name="title" optional="true"/>
      <orm:basic access="PROPERTY" name="persona" optional="true"/>
      <orm:basic name="feature" optional="false">
        <orm:enumerated>STRING</orm:enumerated>
      </orm:basic>
      <orm:many-to-one access="PROPERTY" name="personaGroup" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.play.PersonaGroupType">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.play.PersonaeType" name="PersonaeType">
    <orm:attributes>
      <orm:one-to-many name="group" orphan-removal="true" target-entity="org.eclipse.emf.texo.test.model.samples.play.PersonaeTypeGroupFeatureGroup">
        <orm:join-table/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.play.PersonaGroupType" name="PersonaGroupType">
    <orm:attributes>
      <orm:basic name="groupDescription" optional="false"/>
      <orm:element-collection name="persona">
        <orm:order-column/>
        <orm:collection-table name="PersonaGroupType_persona"/>
      </orm:element-collection>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.play.PlayType" name="PlayType">
    <orm:attributes>
      <orm:basic name="title" optional="false"/>
      <orm:basic name="sceneDescription" optional="false"/>
      <orm:basic name="playSubTitle" optional="false"/>
      <orm:many-to-one name="fm" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.play.FmType">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="personae" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.play.PersonaeType">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:one-to-many name="act" target-entity="org.eclipse.emf.texo.test.model.samples.play.ActType">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.play.SpeechTypeGroupFeatureGroup" name="SpeechType_group">
    <orm:attributes>
      <orm:basic access="PROPERTY" name="speaker" optional="true"/>
      <orm:basic access="PROPERTY" name="line" optional="true"/>
      <orm:basic access="PROPERTY" name="stageDirections" optional="true"/>
      <orm:basic name="feature" optional="false">
        <orm:enumerated>STRING</orm:enumerated>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.play.SpeechType" name="SpeechType">
    <orm:attributes>
      <orm:one-to-many name="group" orphan-removal="true" target-entity="org.eclipse.emf.texo.test.model.samples.play.SpeechTypeGroupFeatureGroup">
        <orm:join-table/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>