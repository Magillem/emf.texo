<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.rental.RentalBicycle" name="RentalBicycle">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes>
      <orm:basic name="type" optional="false">
        <orm:column name="type"/>
      </orm:basic>
      <orm:many-to-one name="rentalCar">
        <orm:join-column name="rentalCar"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.rental.RentalUnit" name="RentalUnit">
    <orm:table name="RentalUnit"/>
    <orm:attributes>
      <orm:basic name="description" optional="false">
        <orm:column name="description"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.rental.RentalCar" name="RentalCar">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes>
      <orm:basic name="size" optional="false">
        <orm:column name="size"/>
      </orm:basic>
      <orm:one-to-one name="currentDriver">
        <orm:join-column name="currentDriver"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:one-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.rental.RentalBusiness" name="RentalBusiness">
    <orm:table name="RentalBusiness"/>
    <orm:attributes>
      <orm:basic name="name" optional="false">
        <orm:column name="name"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.rental.RentalCarDriver" name="RentalCarDriver">
    <orm:table name="RentalCarDriver"/>
    <orm:attributes>
      <orm:basic name="name" optional="false">
        <orm:column name="name"/>
      </orm:basic>
      <orm:one-to-one mapped-by="currentDriver" name="currentRentalCar">
        <orm:join-column name="currentRentalCar"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:one-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.rental.RentalContract" name="RentalContract">
    <orm:table name="RentalContract"/>
    <orm:attributes>
      <orm:basic name="rentToBusinessPartner" optional="false">
        <orm:column name="rentToBusinessPartner"/>
      </orm:basic>
      <orm:basic name="startDate" optional="false">
        <orm:column name="startDate"/>
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="endDate" optional="false">
        <orm:column name="endDate"/>
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="cost" optional="false">
        <orm:column name="cost"/>
      </orm:basic>
      <orm:one-to-many name="rentalContractLines">
        <orm:join-column name="RentalContract_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.rental.RentalContractLine" name="RentalContractLine">
    <orm:table name="RentalContractLine"/>
    <orm:attributes>
      <orm:basic name="number">
        <orm:column name="number"/>
      </orm:basic>
      <orm:many-to-one name="rentalContract">
        <orm:join-column name="rentalContract"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="rentalUnit">
        <orm:join-column name="rentalUnit"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>