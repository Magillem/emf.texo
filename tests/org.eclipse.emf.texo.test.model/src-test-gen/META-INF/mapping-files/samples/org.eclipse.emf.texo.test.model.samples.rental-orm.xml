<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.rental.RentalBicycle" name="RentalBicycle">
    <orm:attributes>
      <orm:basic name="type" optional="false"/>
      <orm:many-to-one name="rentalCar" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.rental.RentalCar">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.rental.RentalUnit" name="RentalUnit">
    <orm:attributes>
      <orm:basic name="description" optional="false"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.rental.RentalCar" name="RentalCar">
    <orm:attributes>
      <orm:basic name="size" optional="false"/>
      <orm:one-to-one name="currentDriver" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.rental.RentalCarDriver">
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:one-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.rental.RentalBusiness" name="RentalBusiness">
    <orm:attributes>
      <orm:basic name="name" optional="false"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.rental.RentalCarDriver" name="RentalCarDriver">
    <orm:attributes>
      <orm:basic name="name" optional="false"/>
      <orm:one-to-one mapped-by="currentDriver" name="currentRentalCar" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.rental.RentalCar">
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:one-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.rental.RentalContract" name="RentalContract">
    <orm:attributes>
      <orm:basic name="rentToBusinessPartner" optional="false"/>
      <orm:basic name="startDate" optional="false">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="endDate" optional="false">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="cost" optional="false"/>
      <orm:one-to-many name="rentalContractLines" target-entity="org.eclipse.emf.texo.test.model.samples.rental.RentalContractLine">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.rental.RentalContractLine" name="RentalContractLine">
    <orm:attributes>
      <orm:basic name="number" optional="true"/>
      <orm:many-to-one name="rentalContract" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.rental.RentalContract">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="rentalUnit" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.rental.RentalUnit">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>