<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.employee.Department" name="Department">
    <orm:attributes>
      <orm:basic name="name" optional="false"/>
      <orm:one-to-many name="employees" target-entity="org.eclipse.emf.texo.test.model.samples.employee.Employee">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.employee.Employee" name="Employee">
    <orm:attributes>
      <orm:basic name="name" optional="false"/>
      <orm:basic name="salary" optional="false"/>
      <orm:basic name="age" optional="false"/>
      <orm:basic name="hireDate" optional="false">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>