<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.library.Book" name="library_Book">
    <orm:primary-key-join-column name="library_Book_parent_id"/>
    <orm:attributes>
      <orm:basic name="title">
        <orm:column name="title"/>
      </orm:basic>
      <orm:basic name="pages">
        <orm:column name="pages"/>
      </orm:basic>
      <orm:basic name="category">
        <orm:column name="category"/>
      </orm:basic>
      <orm:many-to-one name="author" optional="false">
        <orm:join-column name="library_Book_author"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.library.Library" name="library_Library">
    <orm:primary-key-join-column name="library_Library_parent_id"/>
    <orm:attributes>
      <orm:basic name="name">
        <orm:column name="T_name"/>
      </orm:basic>
      <orm:one-to-many name="writers" orphan-removal="true">
        <orm:order-column name="library_Library_writers_ind"/>
        <orm:join-column name="library_Library_writers"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="books" orphan-removal="true">
        <orm:order-column name="library_Library_books_ind"/>
        <orm:join-column name="library_Library_books"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.library.Writer" name="library_Writer">
    <orm:primary-key-join-column name="library_Writer_parent_id"/>
    <orm:attributes>
      <orm:basic name="name">
        <orm:column name="T_name"/>
      </orm:basic>
      <orm:one-to-many name="books">
        <orm:order-column name="library_Writer_books_ind"/>
        <orm:join-table name="library_Writer_books">
          <orm:join-column name="library_Writer_books"/>
          <orm:inverse-join-column name="library_Book_author"/>
        </orm:join-table>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>