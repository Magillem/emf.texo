<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.library.Book" name="Book">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes>
      <orm:basic name="title" optional="true">
        <orm:column name="title"/>
      </orm:basic>
      <orm:basic name="pages" optional="true">
        <orm:column name="pages"/>
      </orm:basic>
      <orm:basic name="category" optional="true">
        <orm:column name="category"/>
      </orm:basic>
      <orm:many-to-one name="author" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.library.Writer">
        <orm:join-column name="author"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.library.Library" name="Library">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes>
      <orm:basic name="name" optional="true">
        <orm:column name="name"/>
      </orm:basic>
      <orm:one-to-many name="writers" orphan-removal="true" target-entity="org.eclipse.emf.texo.test.model.samples.library.Writer">
        <orm:order-column name="ind"/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="books" orphan-removal="true" target-entity="org.eclipse.emf.texo.test.model.samples.library.Book">
        <orm:order-column name="ind"/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.library.Writer" name="Writer">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes>
      <orm:basic name="name" optional="true">
        <orm:column name="name"/>
      </orm:basic>
      <orm:one-to-many name="books" target-entity="org.eclipse.emf.texo.test.model.samples.library.Book">
        <orm:order-column name="ind"/>
        <orm:join-table name="Writer_books">
          <orm:join-column name="Writer_id"/>
          <orm:inverse-join-column name="Book_id"/>
        </orm:join-table>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>