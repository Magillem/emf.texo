<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.capa.CapacityEntry" name="CapacityEntry">
    <orm:table name="CapacityEntry"/>
    <orm:attributes>
      <orm:basic name="capacityEntryId">
        <orm:column name="capacityEntryId"/>
      </orm:basic>
      <orm:basic name="name">
        <orm:column name="name"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.capa.Machine" name="Machine">
    <orm:table name="Machine"/>
    <orm:attributes>
      <orm:basic name="machineId">
        <orm:column name="machineId"/>
      </orm:basic>
      <orm:basic name="machineSearchString">
        <orm:column name="machineSearchString"/>
      </orm:basic>
      <orm:basic name="multiplicity">
        <orm:column name="multiplicity"/>
      </orm:basic>
      <orm:basic name="taskSearchString">
        <orm:column name="taskSearchString"/>
      </orm:basic>
      <orm:many-to-one name="workWeek" optional="false">
        <orm:join-column name="workWeek"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:one-to-many name="tasks">
        <orm:join-column name="Machine_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.capa.WorkWeek" name="WorkWeek">
    <orm:table name="WorkWeek"/>
    <orm:attributes>
      <orm:basic name="workWeekId">
        <orm:column name="workWeekId"/>
      </orm:basic>
      <orm:one-to-many name="days">
        <orm:join-column name="WorkWeek_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.capa.Task" name="Task">
    <orm:table name="Task"/>
    <orm:attributes>
      <orm:basic name="name">
        <orm:column name="name"/>
      </orm:basic>
      <orm:basic name="taskId">
        <orm:column name="taskId"/>
      </orm:basic>
      <orm:basic name="timePerPice">
        <orm:column name="timePerPice"/>
      </orm:basic>
      <orm:basic name="timePerPreperation">
        <orm:column name="timePerPreperation"/>
      </orm:basic>
      <orm:many-to-one name="capacityEntry" optional="false">
        <orm:join-column name="capacityEntry"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="production" optional="false">
        <orm:join-column name="production"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.capa.MachineList" name="MachineList">
    <orm:table name="MachineList"/>
    <orm:attributes>
      <orm:one-to-many name="machines">
        <orm:join-column name="MachineList_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.capa.Production" name="Production">
    <orm:table name="Production"/>
    <orm:attributes>
      <orm:basic name="article">
        <orm:column name="article"/>
      </orm:basic>
      <orm:basic name="dueTime">
        <orm:column name="dueTime"/>
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="missingAmount">
        <orm:column name="missingAmount"/>
      </orm:basic>
      <orm:basic name="productionId">
        <orm:column name="productionId"/>
      </orm:basic>
      <orm:basic name="startTime">
        <orm:column name="startTime"/>
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="totalAmount">
        <orm:column name="totalAmount"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.capa.WorkDay" name="WorkDay">
    <orm:table name="WorkDay"/>
    <orm:attributes>
      <orm:basic name="dayOfWeek">
        <orm:column name="dayOfWeek"/>
      </orm:basic>
      <orm:basic name="duration">
        <orm:column name="duration"/>
      </orm:basic>
      <orm:basic name="startTime">
        <orm:column name="startTime"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>