<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.capa.CapacityEntry" name="CapacityEntry">
    <orm:attributes>
      <orm:basic name="capacityEntryId" optional="true"/>
      <orm:basic name="name" optional="true"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.capa.Machine" name="Machine">
    <orm:attributes>
      <orm:basic name="machineId" optional="true"/>
      <orm:basic name="machineSearchString" optional="true"/>
      <orm:basic name="multiplicity" optional="true"/>
      <orm:basic name="taskSearchString" optional="true"/>
      <orm:many-to-one name="workWeek" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.capa.WorkWeek">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:one-to-many name="tasks" target-entity="org.eclipse.emf.texo.test.model.samples.capa.Task">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.capa.WorkWeek" name="WorkWeek">
    <orm:attributes>
      <orm:basic name="workWeekId" optional="true"/>
      <orm:one-to-many name="days" target-entity="org.eclipse.emf.texo.test.model.samples.capa.WorkDay">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.capa.Task" name="Task">
    <orm:attributes>
      <orm:basic name="name" optional="true"/>
      <orm:basic name="taskId" optional="true"/>
      <orm:basic name="timePerPice" optional="true"/>
      <orm:basic name="timePerPreperation" optional="true"/>
      <orm:many-to-one name="capacityEntry" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.capa.CapacityEntry">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="production" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.capa.Production">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.capa.MachineList" name="MachineList">
    <orm:attributes>
      <orm:one-to-many name="machines" target-entity="org.eclipse.emf.texo.test.model.samples.capa.Machine">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.capa.Production" name="Production">
    <orm:attributes>
      <orm:basic name="article" optional="true"/>
      <orm:basic name="dueTime" optional="true">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="missingAmount" optional="true"/>
      <orm:basic name="productionId" optional="true"/>
      <orm:basic name="startTime" optional="true">
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="totalAmount" optional="true"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.capa.WorkDay" name="WorkDay">
    <orm:attributes>
      <orm:basic name="dayOfWeek" optional="true"/>
      <orm:basic name="duration" optional="true"/>
      <orm:basic name="startTime" optional="true"/>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>