<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.catalog.CatalogType" name="CatalogType">
    <orm:table name="CatalogType"/>
    <orm:attributes>
      <orm:basic name="name" optional="false">
        <orm:column name="name"/>
      </orm:basic>
      <orm:basic name="description" optional="false">
        <orm:column name="description"/>
      </orm:basic>
      <orm:many-to-many name="subCatalog">
        <orm:join-table name="CatalogType_subCatalog">
          <orm:join-column name="CatalogType_id"/>
          <orm:inverse-join-column name="CatalogType_id"/>
        </orm:join-table>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
      <orm:many-to-many name="product">
        <orm:join-table name="CatalogType_product">
          <orm:join-column name="CatalogType_id"/>
          <orm:inverse-join-column name="ProductType_id"/>
        </orm:join-table>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.catalog.ProductType" name="ProductType">
    <orm:table name="ProductType"/>
    <orm:attributes>
      <orm:basic name="code" optional="false">
        <orm:column name="code"/>
      </orm:basic>
      <orm:basic name="description" optional="false">
        <orm:column name="description"/>
      </orm:basic>
      <orm:many-to-one name="price" optional="false">
        <orm:join-column name="price"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:one-to-many name="remark">
        <orm:join-column name="ProductType_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.catalog.PriceType" name="PriceType">
    <orm:table name="PriceType"/>
    <orm:attributes>
      <orm:basic name="price" optional="false">
        <orm:column name="price"/>
      </orm:basic>
      <orm:basic name="quantityTo" optional="false">
        <orm:column name="quantityTo"/>
      </orm:basic>
      <orm:many-to-one name="supplier" optional="false">
        <orm:join-column name="supplier"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.catalog.SupplierType" name="SupplierType">
    <orm:table name="SupplierType"/>
    <orm:attributes>
      <orm:basic name="name" optional="false">
        <orm:column name="name"/>
      </orm:basic>
      <orm:basic name="noOfEmployees" optional="false">
        <orm:column name="noOfEmployees"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.catalog.StringType" name="StringType">
    <orm:table name="StringType"/>
    <orm:attributes>
      <orm:basic name="value" optional="false">
        <orm:column name="value"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>