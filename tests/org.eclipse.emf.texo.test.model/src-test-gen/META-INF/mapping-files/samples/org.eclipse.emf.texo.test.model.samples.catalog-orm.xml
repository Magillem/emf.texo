<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.catalog.CatalogType" name="CatalogType">
    <orm:attributes>
      <orm:basic name="name" optional="false"/>
      <orm:basic name="description" optional="false"/>
      <orm:many-to-many name="subCatalog" target-entity="org.eclipse.emf.texo.test.model.samples.catalog.CatalogType">
        <orm:order-column/>
        <orm:join-table name="CatalogType_subCatalog"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
      <orm:many-to-many name="product" target-entity="org.eclipse.emf.texo.test.model.samples.catalog.ProductType">
        <orm:order-column/>
        <orm:join-table name="CatalogType_product"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.catalog.ProductType" name="ProductType">
    <orm:attributes>
      <orm:basic name="code" optional="false"/>
      <orm:basic name="description" optional="false"/>
      <orm:many-to-one name="price" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.catalog.PriceType">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:one-to-many name="remark" target-entity="org.eclipse.emf.texo.test.model.samples.catalog.StringType">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.catalog.PriceType" name="PriceType">
    <orm:attributes>
      <orm:basic name="price" optional="false"/>
      <orm:basic name="quantityTo" optional="false"/>
      <orm:many-to-one name="supplier" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.catalog.SupplierType">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.catalog.SupplierType" name="SupplierType">
    <orm:attributes>
      <orm:basic name="name" optional="false"/>
      <orm:basic name="noOfEmployees" optional="false"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.catalog.StringType" name="StringType">
    <orm:attributes>
      <orm:basic name="value" optional="false"/>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>