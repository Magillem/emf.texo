<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.workflow.ConditionalOutputPort" name="ConditionalOutputPort">
    <orm:attributes>
      <orm:basic name="condition">
        <orm:column name="COND"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.workflow.Workflow" name="Workflow">
    <orm:attributes>
      <orm:one-to-many name="nodes" target-entity="org.eclipse.emf.texo.test.model.samples.workflow.WorkflowNode">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="edges" target-entity="org.eclipse.emf.texo.test.model.samples.workflow.Edge">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="comments" target-entity="org.eclipse.emf.texo.test.model.samples.workflow.Comment">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.workflow.WorkflowElement" name="WorkflowElement">
    <orm:attributes>
      <orm:basic name="name" optional="true"/>
      <orm:basic name="comment" optional="true"/>
      <orm:basic name="x" optional="true"/>
      <orm:basic name="y" optional="true"/>
      <orm:basic name="width" optional="true"/>
      <orm:basic name="height" optional="true"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.workflow.WorkflowNode" name="WorkflowNode">
    <orm:attributes>
      <orm:basic name="isStart" optional="false"/>
      <orm:basic name="isFinish" optional="false"/>
      <orm:many-to-one name="workflow" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.workflow.Workflow">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:one-to-many name="outputs" target-entity="org.eclipse.emf.texo.test.model.samples.workflow.OutputPort">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="inputs" target-entity="org.eclipse.emf.texo.test.model.samples.workflow.InputPort">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.workflow.Edge" name="Edge">
    <orm:attributes>
      <orm:many-to-one name="workflow" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.workflow.Workflow">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="target" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.workflow.InputPort">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="source" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.workflow.OutputPort">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.workflow.Comment" name="Comment">
    <orm:attributes>
      <orm:many-to-one name="workflow" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.workflow.Workflow">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.workflow.OutputPort" name="OutputPort">
    <orm:attributes>
      <orm:many-to-one name="node" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.workflow.WorkflowNode">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:one-to-many name="edges" target-entity="org.eclipse.emf.texo.test.model.samples.workflow.Edge">
        <orm:order-column/>
        <orm:join-table name="OutputPort_edges"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.workflow.Port" name="Port">
    <orm:attributes/>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.workflow.InputPort" name="InputPort">
    <orm:attributes>
      <orm:many-to-one name="node" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.workflow.WorkflowNode">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:one-to-many name="edges" target-entity="org.eclipse.emf.texo.test.model.samples.workflow.Edge">
        <orm:order-column/>
        <orm:join-table name="InputPort_edges"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.workflow.Fault" name="Fault">
    <orm:attributes/>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.workflow.CompoundTask" name="CompoundTask">
    <orm:attributes>
      <orm:many-to-one name="subworkflow" optional="false" target-entity="org.eclipse.emf.texo.test.model.samples.workflow.Workflow">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.workflow.TransformationTask" name="TransformationTask">
    <orm:attributes>
      <orm:basic name="transformExpression" optional="true"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.workflow.ConditionalTask" name="ConditionalTask">
    <orm:attributes/>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.workflow.LoopTask" name="LoopTask">
    <orm:attributes>
      <orm:basic name="whileCondition" optional="false"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.workflow.Task" name="Task">
    <orm:attributes/>
  </orm:entity>
</orm:entity-mappings>