<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.featuremaptest.PurchaseOrder" name="PurchaseOrder">
    <orm:table name="PUR_ORDER"/>
    <orm:attributes>
      <orm:basic name="name" optional="false"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.featuremaptest.SupplierOrdersFeatureGroup" name="Supplier_orders">
    <orm:attributes>
      <orm:basic name="hardCopyOrderReference" optional="true"/>
      <orm:basic name="hardCopyOrderNumber" optional="true"/>
      <orm:many-to-one name="preferredOrders" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.featuremaptest.PurchaseOrder">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="standardOrders" optional="true" target-entity="org.eclipse.emf.texo.test.model.samples.featuremaptest.PurchaseOrder">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.featuremaptest.Supplier" name="Supplier">
    <orm:table name="SUP_TABLE"/>
    <orm:attributes>
      <orm:basic name="name" optional="false"/>
      <orm:one-to-many name="orders" orphan-removal="true" target-entity="org.eclipse.emf.texo.test.model.samples.featuremaptest.SupplierOrdersFeatureGroup">
        <orm:join-table/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>