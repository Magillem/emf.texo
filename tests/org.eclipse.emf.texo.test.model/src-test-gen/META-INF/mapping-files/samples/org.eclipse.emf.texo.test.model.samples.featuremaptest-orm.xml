<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.featuremaptest.PurchaseOrder" name="PurchaseOrder">
    <orm:table name="PUR_ORDER"/>
    <orm:attributes>
      <orm:basic name="name" optional="false">
        <orm:column name="name"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.featuremaptest.SupplierOrdersFeatureGroup" name="Supplier_orders">
    <orm:attributes>
      <orm:basic name="hardCopyOrderReference">
        <orm:column name="hardCopyOrderReference"/>
      </orm:basic>
      <orm:basic name="hardCopyOrderNumber">
        <orm:column name="hardCopyOrderNumber"/>
      </orm:basic>
      <orm:basic name="feature" optional="false">
        <orm:enumerated>STRING</orm:enumerated>
      </orm:basic>
      <orm:many-to-one name="preferredOrders">
        <orm:join-column name="preferredOrders"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="standardOrders">
        <orm:join-column name="standardOrders"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.featuremaptest.Supplier" name="Supplier">
    <orm:table name="SUP_TABLE"/>
    <orm:attributes>
      <orm:basic name="name" optional="false">
        <orm:column name="name"/>
      </orm:basic>
      <orm:one-to-many name="orders" orphan-removal="true" target-entity="org.eclipse.emf.texo.test.model.samples.featuremaptest.SupplierOrdersFeatureGroup">
        <orm:join-table/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>