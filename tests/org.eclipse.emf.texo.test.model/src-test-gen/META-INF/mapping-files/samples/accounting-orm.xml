<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.accounting.AccountingClass" name="Accounting">
    <orm:table name="Accounting"/>
    <orm:attributes>
      <orm:basic name="name" optional="false">
        <orm:column name="name"/>
      </orm:basic>
      <orm:many-to-one name="vatAccount">
        <orm:join-column name="vatAccount"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="report">
        <orm:join-column name="report"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:one-to-many name="accountGroup">
        <orm:join-column name="Accounting_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="vat">
        <orm:join-column name="Accounting_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="journalGroup">
        <orm:join-column name="Accounting_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.accounting.Account" name="Account">
    <orm:table name="Account"/>
    <orm:attributes>
      <orm:basic name="name" optional="false">
        <orm:column name="name"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.accounting.AccountGroup" name="AccountGroup">
    <orm:table name="AccountGroup"/>
    <orm:attributes>
      <orm:basic name="name" optional="false">
        <orm:column name="name"/>
      </orm:basic>
      <orm:one-to-many name="account">
        <orm:join-column name="AccountGroup_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.accounting.Vat" name="Vat">
    <orm:table name="Vat"/>
    <orm:attributes>
      <orm:basic name="name" optional="false">
        <orm:column name="name"/>
      </orm:basic>
      <orm:basic name="rate" optional="false">
        <orm:column name="rate"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.accounting.BalanceAccount" name="BalanceAccount">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes>
      <orm:many-to-many name="report">
        <orm:join-table name="BalanceAccount_report">
          <orm:join-column name="BalanceAccount_id"/>
          <orm:inverse-join-column name="ReportGroup_id"/>
        </orm:join-table>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.accounting.Report" name="Report">
    <orm:table name="Report"/>
    <orm:attributes>
      <orm:basic name="name" optional="false">
        <orm:column name="name"/>
      </orm:basic>
      <orm:many-to-one name="debitReportGroup" optional="false">
        <orm:join-column name="debitReportGroup"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="creditReportGroup" optional="false">
        <orm:join-column name="creditReportGroup"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.accounting.JournalGroup" name="JournalGroup">
    <orm:table name="JournalGroup"/>
    <orm:attributes>
      <orm:basic name="name" optional="false">
        <orm:column name="name"/>
      </orm:basic>
      <orm:one-to-many name="journalGroups">
        <orm:join-column name="JournalGroup_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="journalStatements">
        <orm:join-column name="JournalGroup_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.accounting.ReportGroup" name="ReportGroup">
    <orm:table name="ReportGroup"/>
    <orm:attributes>
      <orm:basic name="name" optional="false">
        <orm:column name="name"/>
      </orm:basic>
      <orm:one-to-many name="reportGroup">
        <orm:join-column name="ReportGroup_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:many-to-many name="account">
        <orm:join-table name="ReportGroup_account">
          <orm:join-column name="ReportGroup_id"/>
          <orm:inverse-join-column name="BalanceAccount_id"/>
        </orm:join-table>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.accounting.JournalStatement" name="JournalStatement">
    <orm:table name="JournalStatement"/>
    <orm:attributes>
      <orm:basic name="description" optional="false">
        <orm:column name="description"/>
      </orm:basic>
      <orm:basic name="date" optional="false">
        <orm:column name="date"/>
        <orm:temporal>DATE</orm:temporal>
      </orm:basic>
      <orm:basic name="amount" optional="false">
        <orm:column name="amount"/>
      </orm:basic>
      <orm:many-to-one name="debitAccount" optional="false">
        <orm:join-column name="debitAccount"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="creditAccount" optional="false">
        <orm:join-column name="creditAccount"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="vat">
        <orm:join-column name="vat"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.accounting.PLAccount" name="PLAccount">
    <orm:primary-key-join-column name="parent_id"/>
    <orm:attributes/>
  </orm:entity>
</orm:entity-mappings>