<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.emap.Book" name="Book">
    <orm:table name="EMAP_BOOK"/>
    <orm:attributes>
      <orm:basic name="title" optional="false">
        <orm:column name="title"/>
      </orm:basic>
      <orm:one-to-many name="writers">
        <orm:map-key-column name="ind" table="Book_writers"/>
        <orm:join-table name="Book_writers">
          <orm:join-column name="Book_id"/>
          <orm:inverse-join-column name="StringToWriterMapEntry_id"/>
        </orm:join-table>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.emap.BookStore" name="BookStore">
    <orm:table name="EMAP_BOOKSTORE"/>
    <orm:attributes>
      <orm:basic name="name" optional="false">
        <orm:column name="name"/>
      </orm:basic>
      <orm:one-to-many name="booksByWriter">
        <orm:map-key-column name="ind" table="BookStore_booksByWriter"/>
        <orm:join-table name="BookStore_booksByWriter">
          <orm:join-column name="BookStore_id"/>
          <orm:inverse-join-column name="WriterToBookMapEntry_id"/>
        </orm:join-table>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:element-collection name="keyWords">
        <orm:collection-table name="BookStore_keyWords"/>
      </orm:element-collection>
      <orm:element-collection name="namesByWriter">
        <orm:collection-table name="BookStore_namesByWriter"/>
      </orm:element-collection>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.samples.emap.Writer" name="Writer">
    <orm:table name="EMAP_WRITER"/>
    <orm:attributes>
      <orm:basic name="name" optional="false">
        <orm:column name="name"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>