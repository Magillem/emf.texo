<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.test.model.issues.bz325429_test.Person" name="Person">
    <orm:attributes>
      <orm:basic name="firstname" optional="true"/>
      <orm:basic name="lastname" optional="true"/>
      <orm:many-to-one name="address" optional="false" target-entity="org.eclipse.emf.texo.test.model.issues.bz325429_test.Address">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-many name="contacts" target-entity="org.eclipse.emf.texo.test.model.issues.bz325429_test.Contact">
        <orm:order-column/>
        <orm:join-table name="Person_contacts"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
          <orm:cascade-refresh/>
        </orm:cascade>
      </orm:many-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.issues.bz325429_test.Address" name="Address">
    <orm:attributes>
      <orm:basic name="street" optional="true"/>
      <orm:basic name="city" optional="true"/>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.test.model.issues.bz325429_test.Contact" name="Contact">
    <orm:attributes>
      <orm:basic name="value" optional="true"/>
      <orm:basic name="type" optional="true"/>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>