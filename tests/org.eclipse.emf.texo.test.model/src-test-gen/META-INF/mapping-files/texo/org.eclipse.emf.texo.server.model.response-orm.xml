<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.server.model.response.ErrorType" name="ErrorType">
    <orm:attributes>
      <orm:basic name="errorClass" optional="false"/>
      <orm:basic name="message" optional="false"/>
      <orm:basic name="stackTrace" optional="false"/>
      <orm:many-to-one name="cause" optional="false" target-entity="org.eclipse.emf.texo.server.model.response.ErrorType">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.server.model.response.ResponseType" name="ResponseType">
    <orm:attributes>
      <orm:basic name="status" optional="false"/>
      <orm:basic name="startRow" optional="false"/>
      <orm:basic name="endRow" optional="false"/>
      <orm:basic name="totalRows" optional="false"/>
      <orm:one-to-many name="data" target-entity="org.eclipse.emf.ecore.EObject">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.server.model.response.ResultType" name="ResultType">
    <orm:attributes>
      <orm:one-to-many name="updated" target-entity="org.eclipse.emf.ecore.EObject">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="inserted" target-entity="org.eclipse.emf.ecore.EObject">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="deleted" target-entity="org.eclipse.emf.ecore.EObject">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>