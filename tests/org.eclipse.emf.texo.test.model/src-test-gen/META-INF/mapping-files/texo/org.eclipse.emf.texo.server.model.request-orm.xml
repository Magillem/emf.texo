<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.server.model.request.ActionType" name="ActionType">
    <orm:table name="ActionType"/>
    <orm:attributes>
      <orm:one-to-many name="update">
        <orm:join-column name="ActionType_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="insert">
        <orm:join-column name="ActionType_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="delete">
        <orm:join-column name="ActionType_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.server.model.request.QueryType" name="QueryType">
    <orm:table name="QueryType"/>
    <orm:attributes>
      <orm:basic name="namedQuery">
        <orm:column name="namedQuery"/>
      </orm:basic>
      <orm:basic name="query">
        <orm:column name="query"/>
      </orm:basic>
      <orm:one-to-many name="parameters">
        <orm:join-column name="QueryType_id"/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.server.model.request.Parameter" name="Parameter">
    <orm:table name="Parameter"/>
    <orm:attributes>
      <orm:basic name="name" optional="false">
        <orm:column name="name"/>
      </orm:basic>
      <orm:basic name="value" optional="false">
        <orm:column name="value"/>
      </orm:basic>
      <orm:basic name="type">
        <orm:column name="type"/>
      </orm:basic>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>