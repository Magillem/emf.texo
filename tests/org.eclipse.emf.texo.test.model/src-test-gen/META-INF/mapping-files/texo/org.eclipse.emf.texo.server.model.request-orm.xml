<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.server.model.request.ActionType" name="ActionType">
    <orm:attributes>
      <orm:one-to-many name="update" target-entity="org.eclipse.emf.ecore.EObject">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="insert" target-entity="org.eclipse.emf.ecore.EObject">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="delete" target-entity="org.eclipse.emf.ecore.EObject">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.server.model.request.DocumentRootMixedFeatureGroup" name="DocumentRoot_mixed">
    <orm:attributes>
      <orm:many-to-one name="action" optional="true" target-entity="org.eclipse.emf.texo.server.model.request.ActionType">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
      <orm:many-to-one name="query" optional="true" target-entity="org.eclipse.emf.texo.server.model.request.QueryType">
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:many-to-one>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.server.model.request.DocumentRoot" name="DocumentRoot">
    <orm:attributes>
      <orm:one-to-many name="mixed" orphan-removal="true" target-entity="org.eclipse.emf.texo.server.model.request.DocumentRootMixedFeatureGroup">
        <orm:join-table/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:element-collection name="xMLNSPrefixMap">
        <orm:collection-table name="DocumentRoot_xMLNSPrefixMap"/>
      </orm:element-collection>
      <orm:element-collection name="xSISchemaLocation">
        <orm:collection-table name="DocumentRoot_xSISchemaLocation"/>
      </orm:element-collection>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.server.model.request.QueryType" name="QueryType">
    <orm:attributes>
      <orm:basic name="namedQuery" optional="true"/>
      <orm:basic name="query" optional="true"/>
      <orm:one-to-many name="parameters" target-entity="org.eclipse.emf.texo.server.model.request.Parameter">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.server.model.request.Parameter" name="Parameter">
    <orm:attributes>
      <orm:basic name="name" optional="false"/>
      <orm:basic name="value" optional="false"/>
      <orm:basic name="type" optional="true"/>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>