<?xml version="1.0" encoding="ASCII"?>
<orm:entity-mappings xmlns:orm="http://www.eclipse.org/eclipselink/xsds/persistence/orm" version="2.1">
  <orm:persistence-unit-metadata>
    <orm:persistence-unit-defaults>
      <orm:delimited-identifiers/>
    </orm:persistence-unit-defaults>
  </orm:persistence-unit-metadata>
  <orm:access>FIELD</orm:access>
  <orm:entity class="org.eclipse.emf.texo.server.model.request.ActionType" name="ActionType">
    <orm:attributes>
      <orm:one-to-many name="update" target-entity="org.eclipse.emf.ecore.EObject">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="insert" target-entity="org.eclipse.emf.ecore.EObject">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
      <orm:one-to-many name="delete" target-entity="org.eclipse.emf.ecore.EObject">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.server.model.request.QueryType" name="QueryType">
    <orm:attributes>
      <orm:basic name="namedQuery" optional="true"/>
      <orm:basic name="query" optional="true"/>
      <orm:one-to-many name="parameters" target-entity="org.eclipse.emf.texo.server.model.request.Parameter">
        <orm:order-column/>
        <orm:join-column/>
        <orm:cascade>
          <orm:cascade-all/>
        </orm:cascade>
      </orm:one-to-many>
    </orm:attributes>
  </orm:entity>
  <orm:entity class="org.eclipse.emf.texo.server.model.request.Parameter" name="Parameter">
    <orm:attributes>
      <orm:basic name="name" optional="false"/>
      <orm:basic name="value" optional="false"/>
      <orm:basic name="type" optional="true"/>
    </orm:attributes>
  </orm:entity>
</orm:entity-mappings>